# Mini Jet HR - Environment Variables
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Django settings
DJANGO_SECRET_KEY=change-me-to-a-random-string-at-least-50-chars
DJANGO_DEBUG=True
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1,host.docker.internal

# Database (PostgreSQL) - used by Django
DB_NAME=minijethr
DB_USER=minijethr_user
DB_PASSWORD=change-me
DB_HOST=db
DB_PORT=5432

# PostgreSQL container init - must match DB_* values above
POSTGRES_DB=minijethr
POSTGRES_USER=minijethr_user
POSTGRES_PASSWORD=change-me
